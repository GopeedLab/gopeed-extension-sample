{
  "name": "github-contributor-avatars",
  "author": "gopeed",
  "title": "Github Contributor Avatars",
  "description": "Download github repository contributors avatars",
  "icon": "icon.png",
  "version": "1.0.0",
  "homepage": "https://gopeed.com",
  "repository": {
    "url": "https://github.com/GopeedLab/gopeed-extension-samples",
    "directory": "github-contributor-avatars-sample"
  },
  "scripts": [
    {
      "event": "onResolve",
      "matches": [
        "*://github.com/*/contributors"
      ],
      "entry": "dist/index.js"
    }
  ],
  "settings": [
    {
      "name": "ua",
      "title": "User-Agent",
      "description": "HTTP User-Agent header",
      "type": "string"
    }
  ]
}