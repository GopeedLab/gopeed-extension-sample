{
  "name": "extension-test",
  "author": "gopeed",
  "title": "Gopeed Extension Test",
  "description": "Test extension settings and upgrade",
  "version": "1.0.1",
  "homepage": "https://gopeed.com",
  "repository": "https://github.com/GopeedLab/gopeed-extension-samples",
  "installURL": "https://github.com/GopeedLab/gopeed-extension-samples#extension-test",
  "scripts": [
    {
      "event": "onResolve",
      "matches": [
        "*://test.com/*"
      ],
      "entry": "index.js"
    }
  ],
  "settings": [
    {
      "name": "s1",
      "title": "S1",
      "description": "Test string without default value",
      "type": "string",
      "required": true
    },
    {
      "name": "s2",
      "title": "S2",
      "description": "Test string with default value",
      "type": "string",
      "value": "test"
    },
    {
      "name": "s3",
      "title": "S3",
      "description": "Test string with options",
      "type": "string",
      "value": "test1",
      "options": [
        {
          "label": "Test1",
          "value": "test1"
        },
        {
          "label": "Test2",
          "value": "test2"
        }
      ]
    },
    {
      "name": "n1",
      "title": "N1",
      "description": "Test number without default value",
      "type": "number",
      "required": true
    },
    {
      "name": "n2",
      "title": "N2",
      "description": "Test number with default value",
      "type": "number",
      "value": 1
    },
    {
      "name": "n3",
      "title": "N3",
      "description": "Test number with options",
      "type": "number",
      "value": 1,
      "options": [
        {
          "label": "Test1",
          "value": 1
        },
        {
          "label": "Test2",
          "value": 2
        }
      ]
    },
    {
      "name": "b1",
      "title": "B1",
      "description": "Test boolean without default value",
      "type": "boolean",
      "required": true
    },
    {
      "name": "b2",
      "title": "B2",
      "description": "Test boolean with default value",
      "type": "boolean",
      "value": true
    }
  ]
}